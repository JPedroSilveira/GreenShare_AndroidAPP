<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.si.greenshare">
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.si.greenshare.activity.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.si.greenshare.activity.register.RegisterActivity"
            android:parentActivityName="com.si.greenshare.activity.LoginActivity" />
        <activity
            android:name="com.si.greenshare.activity.register.RegisterStepTwoActivity"
            android:parentActivityName="com.si.greenshare.activity.register.RegisterActivity" />
        <activity
            android:name="com.si.greenshare.activity.register.RegisterStepThreeActivity"
            android:parentActivityName="com.si.greenshare.activity.register.RegisterStepTwoActivity" />
        <activity android:name="com.si.greenshare.activity.register.RegisterConfirm"
            android:parentActivityName="com.si.greenshare.activity.register.RegisterStepThreeActivity"/>
    </application>

</manifest>